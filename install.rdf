<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:name>Acebug</em:name>
    <em:description>Syntax highlighting for the Firebug command line using Ace. Firebug 1.9 or above must be installed in order to use this extension.</em:description>
    <em:id>acebug@mozilla.org</em:id>
    <em:version>1.10.0</em:version>
    <em:type>2</em:type>
    <em:creator>Harutyun Amirjanyan &amp; Michael Ratcliffe</em:creator>
    <em:iconURL>chrome://acebug/content/acebug.png</em:iconURL>
    <em:icon64URL>chrome://acebug/content/acebug64.png</em:icon64URL>
    <em:optionsURL>chrome://acebug/content/options.xul</em:optionsURL>
    <em:homepageURL>http://www.flailingmonkey.com/acebug/</em:homepageURL>

    <!-- Any suitably modern toolkit application -->
    <em:targetApplication>
      <Description>
        <em:id>toolkit@mozilla.org</em:id>
        <em:minVersion>1.9.0.*</em:minVersion>
        <em:maxVersion>17.0a1</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Firefox, at least one valid target application needed by AMO -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>8.0</em:minVersion>
        <em:maxVersion>17.0a1</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:translator>ReinekeFux (de)</em:translator>
    <em:translator>urko (es)</em:translator>
    <em:translator>Goofy (fr)</em:translator>
    <em:translator>markh (nl)</em:translator>
    <em:translator>DakSrbija (sr)</em:translator>
    <em:translator>Lakrits (sv-SE)</em:translator>
    <em:translator>yfdyh000 (zh-CN)</em:translator>
  </Description>
</RDF>